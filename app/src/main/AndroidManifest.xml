<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.studyapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">
        <activity android:name=".Tools.ToolsMenu"></activity>
        <activity android:name=".Help" />
        <activity android:name=".Events.ViewEvents" />
        <activity android:name=".Events.CreateEvent" />
        <activity android:name=".Tools.Timer.TimerMenu" />
        <activity android:name=".Tools.Timer.Timer" />
        <activity android:name=".Tools.StudyTips.StudyTips" />
        <activity android:name=".Tools.MemoryCards.MyCards" />
        <activity android:name=".Tools.MemoryCards.CardMenu" />
        <activity android:name=".Tools.MemoryCards.CreateCards" />
        <activity android:name=".Events.EventsMenu" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>